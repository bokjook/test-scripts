echo "Script to enable cisco switch port";
sw_port_enable()
{
echo "open $switch_ip";
sleep 3;
echo "$user";
sleep 2;
echo "$password";
sleep 2;
echo "config terminal";
sleep 2;
echo "interface $port";
sleep 2;
echo "no shutdown";
sleep 5;
echo "end";
sleep 2;
echo "exit";
}

switch_ip="$1";
user="$2";
password="$3";
port="$4";
sw_port_enable | telnet;
sleep 10;
